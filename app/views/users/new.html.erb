<h1>signup</h1>

<%= render partial: "layouts/errors", locals: {object: @user} %>

<div class="signup-form">
  <%= form_for @user do |f|%>
    <div class="">
      <%= f.label :username %>
      <%= f.text_field :username %>
    </div>
    <br>
    <div class="">
      <%= f.label :password %>
      <%= f.password_field :password %>
    </div>
    <br>
    <%= f.submit "sign up" %>
  <% end %>
</div>
